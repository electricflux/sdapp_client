<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.parking.dashboard"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET" />
    

    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name="com.parking.dashboard.activity.DashboardActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.parking.paymenthistory.PaymentHistory"
                  android:label="@string/app_name">
        </activity>
        
        <activity android:name="com.parking.findparking.FindParking"
                  android:label="@string/app_name">
        </activity>

        <activity android:name="com.parking.locatemycar.LocateMyCar"
                  android:label="@string/app_name">
        </activity>

        <activity android:name="com.parking.payforspot.PayForSpot"
                  android:label="@string/app_name">
        </activity>

      <uses-library android:name="com.google.android.maps" />
    </application>
</manifest>